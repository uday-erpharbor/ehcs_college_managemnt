<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="history_form_view" model="ir.ui.view">
		<field name="name">about.college</field>
		<field name="model">about.college</field>
		<field name="arch" type="xml">
			<form name="History">
				<sheet>
					<group>
						<group>
							<field name="name"/>
							<field name="since"/>
							<field name="city"/>
							<field name="uni"/>
						</group>
					</group>
				</sheet>
			</form>
		</field>
	</record>

